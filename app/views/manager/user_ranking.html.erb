<div class="cont">
<div class="contCreateList">
<div class="contContactIn about">
  <table border="1" width="500" cellspacing="0" cellpadding="5" bordercolor="#333333">
    <tr>
      <th>ユーザーid</th>
      <th>ユーザー名</th>
      <th>件数</th>
    </tr>
    <script>
      var dataset = [];
    </script>

  <% @result.each_with_index do |result, i| %>
  <script>
    var data = [<%= result[0] %>,<%= result[1] %>,<%= result[2] %>]
    dataset.push(data);
  </script>
  <tr>
    <td><%= result[0] %></td>
    <td><%= result[1] %></td>
    <td><%= result[2] %></td>
  </tr>
  <% end %>
</table>
</div>
</div>
</div>
